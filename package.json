{
  "name": "i18next-turbo",
  "version": "0.5.0",
  "description": "Blazing fast i18next translation key extractor - 10-100x faster with Rust + SWC",
  "license": "MIT",
  "author": "i18next-turbo contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/albert-einshutoin/i18next-turbo.git"
  },
  "keywords": [
    "i18next",
    "i18n",
    "internationalization",
    "translation",
    "extractor",
    "parser",
    "rust",
    "swc",
    "fast"
  ],
  "main": "lib/index.js",
  "bin": {
    "i18next-turbo": "./bin/cli.js"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "postinstall": "node bin/postinstall.js",
    "sync-version": "node scripts/sync-version.mjs",
    "benchmark": "node scripts/run-benchmark.mjs",
    "benchmark:ci": "node scripts/run-benchmark.mjs 5 --json benchmarks/latest.json --min-speedup 10",
    "bench:fixtures": "node scripts/generate-bench-fixtures.mjs"
  },
  "dependencies": {
    "cosmiconfig": "^9.0.0",
    "jiti": "^1.21.0"
  },
  "optionalDependencies": {
    "i18next-turbo-darwin-arm64": "0.5.0",
    "i18next-turbo-darwin-x64": "0.5.0",
    "i18next-turbo-linux-arm64": "0.5.0",
    "i18next-turbo-linux-x64": "0.5.0",
    "i18next-turbo-win32-ia32": "0.5.0",
    "i18next-turbo-win32-x64": "0.5.0"
  },
  "engines": {
    "node": ">=14"
  }
}
